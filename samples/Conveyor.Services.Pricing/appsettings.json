{
  "app": {
    "name": "Conveyor Pricing Service"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "pricing-service",
    "address": "docker.for.win.localhost",
    "port": "5003",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "pricing-service"
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "pricing",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": ["/ping", "/metrics"]
  },
  "logger": {
    "applicationName": "pricing-service",
    "excludePaths": ["/ping", "/metrics"],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5
  }
}